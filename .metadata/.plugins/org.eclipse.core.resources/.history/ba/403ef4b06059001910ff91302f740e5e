package com.class18;

public class Minimum {

	static int minOfValues(int [] x) {
        min m = new min();
        
        int my_array[] = {20,2,3,4,5,6,1,8,9,10};
        
        System.out.println(min.minOfValues(my_array));
		return 0;    
    }
    
}

class min {
    
     static int minOfValues(int[] num) {
        
        int min = num[0];
        
        for(int i = 0; i < num.length; i++) {
            
            if(num[i] < min) {
                min = num[i];
            
            }
        }
        return min;
    }
}