package com.class3;

public class JavaInterviewQ {

	public static void main(String[] args) {
		// Write a Java Programe to check whether number is even or odd
		
	     int num = 5;
	     if(num%2==0) {
	    	 System.out.println("Number" + num + "is even. ");;
	     }else {
	    	 System.out.println("Number"+num+ "is odd. ");
	     }

	}

}
