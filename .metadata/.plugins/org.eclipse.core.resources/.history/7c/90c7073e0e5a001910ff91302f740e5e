package com.randomP;

public class Main85 {
	 void printTime(int hour, int minute){
		    System.out.print(hour);
		    System.out.print(":");
		    System.out.println(minute);
		  }
		  public static void main(String[] args) {
		    Main85 mm= new Main85();
		    mm.printTime(11,30);
		    
		  }
		}